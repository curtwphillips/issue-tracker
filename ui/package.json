    "compile": "webpack --mode production",
    "#watch": "Compile, and recompile on any changes",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curtwphillips/issue-tracker.git"
  },
  "author": "curtwphillips@gmail.com",
