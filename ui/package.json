    "bootstrap": "^3.4.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "http-proxy-middleware": "^0.21.0",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^1.18.9",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.13.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.2.0",
    "serialize-javascript": "^3.0.0",
    "source-map-support": "^0.5.9",
